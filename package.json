{
  "name": "peerjs-esnext",
  "version": "1.3.0",
  "description": "PeerJS client",
  "main": "./dist/peerjs.js",
  "module": "./dist/peerjs.js",
  "esnext": "./dist/peerjs.js",
  "browser": "./dist/peerjs.min.js",
  "umd": "./dist/peerjs.min.js",
  "homepage": "https://github.com/eolme/peerjs#readme",
  "keywords": [
    "peerjs",
    "webrtc",
    "p2p",
    "rtc"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/eolme/peerjs.git"
  },
  "bugs": {
    "url": "https://github.com/eolme/peerjs/issues"
  },
  "files": [
    "dist/*.js",
    "lib/",
    "index.d.ts"
  ],
  "author": "Michelle Bu, Eric Zhang, Jairo Caro-Accino, Carlos Caballero",
  "maintainers": [
    "Alex Sosnovskiy <devbyru@gmail.com> (https://github.com/afrokick)",
    "Anton Petrov <im@petrov.engineer> (https://github.com/eolme)"
  ],
  "license": "MIT",
  "scripts": {
    "build": "parcel build lib/exports.ts --out-file peerjs.min.js --experimental-scope-hoisting",
    "build-nonminified": "parcel build lib/exports.ts --out-file peerjs.js --no-minify --experimental-scope-hoisting",
    "prepublishOnly": "npm run build",
    "test": "mocha -r ts-node/register -r jsdom-global/register test/**/*.ts"
  },
  "release": {
    "branch": "master"
  },
  "types": "./index.d.ts",
  "devDependencies": {
    "@types/chai": "^4.2.11",
    "@types/mocha": "^7.0.2",
    "babel-core": "^6.26.3",
    "babel-preset-env": "^1.7.0",
    "chai": "^4.2.0",
    "jsdom": "^16.3.0",
    "jsdom-global": "^3.0.2",
    "mocha": "^7.2.0",
    "mock-socket": "8.0.5",
    "parcel-bundler": "^1.12.4",
    "standard": "^14.3.4",
    "ts-node": "^8.10.2",
    "typescript": "^3.9.6"
  },
  "dependencies": {
    "@types/node": "^14.0.20",
    "@types/webrtc": "^0.0.26",
    "eventemitter3": "^4.0.4",
    "peerjs-js-binarypack": "1.0.1",
    "webrtc-adapter": "^7.6.3"
  },
  "collective": {
    "type": "opencollective",
    "url": "https://opencollective.com/peer"
  }
}